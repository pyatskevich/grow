{
  "name": "grow-app",
  "version": "0.1.0",
  "private": true,
  "main": "webpack.config.js",
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "dotenv": "^6.1.0",
    "eslint": "5.6.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
